{

    "manifest_version": 2,
    "name": "yt_no_recommendations",
    "version": "1.0",
  
    "description": "Displays all recommendations on YouTube",
  
    "content_scripts": [
      {
        "matches": ["*://*.youtube.com/*"],
        "js": ["yt_no_recommendations.js"]
      }      
    ]
  
  }